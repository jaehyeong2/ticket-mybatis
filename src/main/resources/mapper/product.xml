<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jjfactory.ticket.mapper.ProductMapper">

    <select id="findById" resultType="jjfactory.ticket.domain.product.Product">
        SELECT * FROM product
        WHERE product_id = #{product_id}
    </select>

    <select id="findAll" resultType="jjfactory.ticket.domain.product.Product">
        SELECT * FROM product
    </select>

</mapper>